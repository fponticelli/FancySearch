# cleanup
-cmd rm -rf ./docs/xml
-cmd rm -rf ./docs/pages
-cmd mkdir -p ./docs/pages

# build without output
-cp src
-cp docs
-lib thx.core
-D doc-gen
-D dox
-dce no
--no-output
ImportFancy
-xml docs/xml/fancy.xml
-js _

