.fs
  &-search
    &-input
      box-sizing border-box

  &-suggestion
    &-container
      left 0
      position absolute
      right 0

    &-list
      background #fff
      border 1px solid #ddd
      box-shadow 0 1px 2px rgba(0, 0, 0, 0.2)
      box-sizing border-box
      list-style none
      margin 0
      padding 0

    &-item
      color #666
      padding 5px 10px

      &:hover
        background #eee
